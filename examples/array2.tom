let printArr(arr : [Int]) : Unit =
    print("[")
    if len(arr) == 0 then
        println("]")
    else
        let flag = true
        for x in arr do
            if flag then
                print(x)
                flag = false
            else
                print(", ")
                print(x)
            end
        end
        println("]")
    end
end

let test = [true,3]
let test1 = [5,6]
printArr(test ++ test1 ++ 5)
test = test ++ 4
